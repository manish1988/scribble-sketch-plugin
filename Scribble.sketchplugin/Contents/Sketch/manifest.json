{
    "name": "Scribble Sketch Plugin",
    "description": "Sync and generate copy tool from sketch artboards to Scribble",
    "author": "Scribble",
    "homepage": "https://www.canvasflip.com/visual-inspector/scribble/",
    "identifier": "com.canvasflip.scribble_sketch_plugin",
    "updateURL": "https://www.canvasflip.com/visual-inspector/scribble/",
    "appcast": "https://github.com/manish1988/scribble-sketch-plugin/blob/master/appcast.xml",
    "version": "2.0.0",
    "compatibleVersion": 3,
    "bundleVersion": 1,
    "commands": [
        {
            "name": "Sync Artboards To Scribble",
            "identifier": "sync",
            "handler": "syncArtboards",
            "script": "action.js",
            "shortcut": "ctrl shift C"
        },
        {
            "name": "Sync Back Artboards From Scribble",
            "identifier": "sync-back",
            "handler": "syncBackArtboards",
            "script": "action.js",
            "shortcut": "ctrl shift V"
        }
    ],
    "menu": {
        "title": "CanvasFlip Scribble",
        "items": [
            "sync",
            "sync-back",
            "report"
        ]
    }
}